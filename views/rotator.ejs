<!DOCTYPE html>
<html>
  <head>
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdn.betonline.ag/img/mkt/slick.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdn.betonline.ag/img/mkt/slick-theme.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdn.betonline.ag/img/mkt/slick-custom.css"
    />
    <script
      src="https://cdn.betonline.ag/img/mkt/jquery-3.js"
    ></script>
    <script
      type="text/javascript"
      src="https://cdn.betonline.ag/img/mkt/slick.js"
    ></script>
    <script
      type="text/javascript"
      src="https://cdn.betonline.ag/img/mkt/scripts.js"
    ></script>
    <title><%= title %></title>
  </head>
  <body>
    <section id="features" class="blue">
      <div class="content">
        <div class="slider single-item">
          <% slides.forEach(function(slide) { %>
          <div>
            <a href="<%= slide.cta %>">
              <picture>
                <source media="(max-width: 799px)" srcset="<%= slide.mobile.url %>">
                <source media="(min-width: 800px)" srcset="<%= slide.desktop.url %>">
                <img src="<%= slide.desktop.url %>" alt="<%= slide.alt %>" />
              </picture>
            </a>
          </div>
          <% }) %>
        </div>
      </div>
    </section>
  </body>
</html>
